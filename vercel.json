{
  "cleanUrls": true,
  "framework": "vitepress",
  "outputDirectory": "example/dist",
  "rewrites": [
    { "source": "/sitemap.xml", "destination": "/sitemap.xml" },
    { "source": "/feed.xml", "destination": "/feed.xml" },
    { "source": "/:path*", "destination": "/index.html" }
  ],
  "installCommand": "pnpm install",
  "buildCommand": "pnpm run build",
  "engines": {
    "node": ">=18.x"
  },
  "build": {
    "env": {
      "NODE_ENV": "production"
    }
  }
}